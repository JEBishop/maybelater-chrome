{
	"manifest_version": 3,
	"name": "MaybeLater",
	"version": "0.1",
	"permissions": [
	  "contextMenus",
	  "storage",
	  "identity",
	  "identity.email"
	],
	"background": { 
	  "service_worker": "main.js"
	},
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"action": {
		"default_icon": "icon16.png",
		"default_popup": "popup.html"
	},
	"content_security_policy": {
		"extension_pages": "script-src 'self'; object-src 'self'"
	}
}